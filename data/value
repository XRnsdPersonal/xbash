export readonly mRoDirNameLog=log
export readonly mRoDirNameCmd=cmds
export readonly mRoDirNameTools=tools

# 	dirPathCmdsDataValues=$(cd `dirname $0`; pwd)/
# export readonly mRoDirPathCmds=$(dirname $dirPathCmdsDataValues)
