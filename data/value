export readonly mRoFileNameCmdModuleTest=test.sh
export readonly mRoFileNameCmdModuleTools=tools.sh
export readonly mRoFileNameCmdModuleCompile=compile.sh
export readonly mRoFileNameCmdModuleSbr=system_backup_restore.sh
export readonly mRoDirNameCmdData=data/

#外部参数
export readonly mRoBaseShellParameter1=$0
export readonly mRoBaseShellParameter2=$1
export readonly mRoBaseShellParameter3=$2

export readonly mRoTimeBackup=$(date -d "today" +"%m%d-%H:%M:%S")
export mTimingStart

export readonly mRoDirPathTools=${mRoDirPathUserHome}${mRoDirNameTools}
export readonly mRoDirPathCmdModule=${mRoDirPathUserHome}${mRoDirNameCmd}/${mRoDirNameCmdModule}

